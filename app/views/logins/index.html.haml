%h1 Listing logins

%table
  %thead
    %tr
      %th Email
      %th Password
      %th User
      %th
      %th
      %th

  %tbody
    - @logins.each do |login|
      %tr
        %td= login.email
        %td= login.password
        %td= login.user
        %td= link_to 'Show', login
        %td= link_to 'Edit', edit_login_path(login)
        %td= link_to 'Destroy', login, method: :delete, data: { confirm: 'Are you sure?' }

%br

= link_to 'New Login', new_login_path
